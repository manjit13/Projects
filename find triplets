function triplets(arr) {
   let res=0
    for(i=0;i<arr.length;i++){
        for(j=i;j<arr.length;j++) {
            if(arr[j]>arr[i]) {
                for(k=j;k<arr.length;k++) {
                   if(arr[k]>arr[j]) {console.log(arr[i], arr[j], arr[k]); res++}
                }
            }
          }
    }
}
